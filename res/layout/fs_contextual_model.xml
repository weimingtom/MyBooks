<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/fun_contextual_model"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:orientation="vertical"
  android:gravity="center"
  android:layout_gravity="center"
  android:visibility="gone"
	  >
	  	<LinearLayout
	  	  android:layout_width="fill_parent"
		  android:layout_height="wrap_content"
		  android:gravity="center"
		  android:layout_gravity="center"
		  android:layout_weight="1"
		  android:background="@drawable/fs_contextual_model"
		  android:orientation="vertical">
			  <LinearLayout
		  	  android:layout_width="fill_parent"
			  android:layout_height="fill_parent"
			  android:gravity="center"
			  android:layout_weight="1"
			  android:layout_gravity="center|top"
			  android:layout_marginTop="20dp"
			  android:orientation="horizontal">
		  		<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:text="主题设置">
					</TextView>
			</LinearLayout>
	  	
			<LinearLayout
		  	  android:layout_width="fill_parent"
			  android:layout_height="fill_parent"
			  android:gravity="center"
			  android:layout_weight="1"
			  android:orientation="horizontal">
			  	<LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_bescon"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_bescon_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="梦幻绿">
					</TextView>
			  	</LinearLayout>
			  	<LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_mattpurple"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_mattpurple_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="哑光紫">
					</TextView>
			  	</LinearLayout>
			  	<LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_freshblue"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_freshblue_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="清新蓝">
					</TextView>
			  	</LinearLayout>
			</LinearLayout>	
			<LinearLayout
		  	  android:layout_width="fill_parent"
			  android:layout_height="fill_parent"
			  android:gravity="center"
			  android:layout_weight="1"
			  android:layout_gravity="center|top"
			  android:orientation="horizontal">
			  <LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_elegantpowder"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_elegantpowder_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="淡雅粉">
					</TextView>
			  	</LinearLayout>
			  	<LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_elegantpurple"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_elegantpurple_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="淡雅紫">
					</TextView>
			  	</LinearLayout>
			  	<LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_elegantyellow"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_elegantyellow_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="淡雅黄">
					</TextView>
			  	</LinearLayout>
			</LinearLayout>	
			<LinearLayout
		  	  android:layout_width="fill_parent"
			  android:layout_height="fill_parent"
			  android:gravity="center"
			  android:layout_weight="1"
			  android:layout_gravity="center|top"
			  android:layout_marginBottom="20dp"
			  android:orientation="horizontal">
			   <LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_elegantblue"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_elegantblue_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="淡雅蓝">
					</TextView>
			  	</LinearLayout>
			  	<LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_elegantwhite"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_elegantwhite_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="淡雅白">
					</TextView>
			  	</LinearLayout>
			  	<LinearLayout
			  	  android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:orientation="horizontal">
			  		<Button
			  			android:id="@+id/btn_blacknight"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_weight="1"
						android:layout_gravity="center_vertical|right"
						android:gravity="center"
						android:background="@drawable/fs_blacknight_style">
					</Button>
					<TextView
				  	android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center"
					android:paddingTop="20dp"
					android:layout_weight="1"
					android:layout_gravity="center"
					android:textColor="#ffffff"
					android:textSize="18dp"
					android:paddingRight="10dp"
					android:paddingLeft="10dp"
					android:text="夜间黑">
					</TextView>
			  	</LinearLayout>
			</LinearLayout>	
		</LinearLayout>
</FrameLayout>
