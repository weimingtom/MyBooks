<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent">

	<Button android:id="@+id/btBook" android:layout_height="120dp"
		android:layout_marginBottom="10dp" android:maxLines="3"
		android:layout_width="90dp" android:layout_marginTop="10dp"
		android:layout_gravity="top" android:layout_marginLeft="5dp"></Button>

	<ImageView android:layout_width="25dp" android:id="@+id/ivdel"
		android:layout_height="25dp" android:src="@drawable/shelldel"></ImageView>

	<ImageView android:layout_width="90dp" android:id="@+id/forcebackground"
		android:layout_height="120dp" android:background="@drawable/btn_book_style"
		android:layout_marginTop="10dp" android:layout_marginBottom="10dp"
		android:layout_gravity="top" android:layout_marginLeft="5dp"
		android:visibility="visible" android:longClickable="true"></ImageView>
</FrameLayout>
